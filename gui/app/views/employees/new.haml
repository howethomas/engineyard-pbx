%h2 New employee

= error_messages_for :employee

- form_for(@employee) do |f|
  %p
    %b Name
    %br
    = f.text_field :name

  %p
    %b Time zone
    %br
    = f.text_field :time_zone

  %p
    %b Extension
    %br
    = f.text_field :extension

  %p
    = f.submit "Create"

- end

= link_to 'Back', employees_path
